@model MvcAssessement2.Models.Movies
@{
    ViewBag.Title = "ByDirector";
}

<h2>Movies by Director</h2>
<ul class="list-group">
    @foreach (var m in Model)
     {
        <li class="list-group-item">
            <strong>@m.Moviename</strong> — Released: @m.DateofRelease.ToShortDateString()
        </li>
     }
</ul>

